# Required
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?pgbouncer=true"
JWT_SECRET="your-secret-key-min-32-chars"

# Optional — direct connection used by prisma migrate (no pgbouncer)
DIRECT_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Optional — allowed CORS origin (default: http://localhost:3000)
FRONTEND_URL="http://localhost:3000"

# Required — Supabase Storage (no SDK needed, uses REST API)
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Optional — Redis cache (default: redis://localhost:6379)
REDIS_URL="redis://localhost:6379"

# Optional — server port (default: 4000)
PORT=4000
